            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="1dp"
            android:layout_marginStart="1dp"
            android:layout_weight="2"
            android:padding="10dp"
            android:text="@string/customer_number"
            android:textColor="@android:color/white"
            android:textStyle="bold"
            />

        <TextView
            android:id="@+id/tv_customer_drop_phone"
            android:layout_width="226dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_gravity="right"
            android:layout_marginEnd="3dp"
            android:layout_marginRight="3dp"
            android:background="@drawable/ic_rectangle_white"
            android:gravity="center_vertical|start"
            android:hint="@string/customer_number"
            android:maxLines="1"
            android:padding="@dimen/margin8dp"
            android:textColor="@android:color/black" />




    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="113dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="1dp"
            android:layout_marginStart="1dp"
            android:layout_weight="2"
            android:padding="10dp"
            android:text="@string/delivery"
            android:textColor="@android:color/white"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tv_delivery"
            android:layout_width="227dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:layout_gravity="right"
            android:layout_marginEnd="3dp"
            android:background="@drawable/ic_rectangle_white"
            android:gravity="center_vertical|start"
            android:hint="@string/delivery"
            android:maxLines="1"
            android:padding="@dimen/margin8dp"
            android:textColor="@android:color/black"
