            android:hint="@string/customer_name"
            android:maxLines="1"
            android:padding="@dimen/margin8dp"
            android:textColor="@android:color/black"
            android:layout_alignParentRight="true"
            android:layout_marginRight="3dp" />


    </RelativeLayout>

    <RelativeLayout
